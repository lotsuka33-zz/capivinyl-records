<div class="container">
  <h2>Publish a New Vinyl</h2>
  <br>
  <% if @vinyl.errors.any? %>
    <p>You have the following errors:</p>
    <%= @vinyl.errors.messages %>
  <% end %>
  <div class="form col-xs-6 col-xs-offset-3">
    <%= render 'form', vinyl: @vinyl %>
    <br>
    <button class="btn btn-info"><%= link_to 'Back', vinyls_path %></button>
  </div>
</div>
