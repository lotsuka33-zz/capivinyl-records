  <%= simple_form_for [@vinyl] do |f| %>
  <%= f.input :artist %>
  <%= f.input :album %>
  <%= f.input :year %>
  <%= f.input :genre %>
  <%= f.input :description %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.input :available, as: :boolean, input_html: { checked: true } %>


  <%= f.button :submit, class: "btn btn-primary" %>
<% end %>
