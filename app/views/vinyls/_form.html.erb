  <%= simple_form_for [@vinyl] do |f| %>
  <%= f.input :artist %>
  <%= f.input :album %>
  <%= f.input :year %>
  <%= f.input :genre %>
  <%= f.input :description %>
  <%= f.input :picture %>
  <%= f.input :available, as: :boolean, checked_value: true, unchecked_value: false %>


  <%= f.button :submit, class: "btn btn-primary" %>
<% end %>
